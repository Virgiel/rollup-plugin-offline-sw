{
  "name": "rollup-plugin-offline-sw",
  "description": "A rollup plugin to generate service worker script for offline access of your static website",
  "version": "0.3.0-alpha.1",
  "repository": "Virgiel/rollup-plugin-offline-sw",
  "module": "lib/index.mjs",
  "main": "lib/index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./lib/index.mjs"
    },
    "./manager": {
      "require": "./manager/index.js",
      "import": "./manager/index.mjs"
    }
  },
  "scripts": {
    "build": "bundt",
    "test": "npm run build && node -r esm test/index.test.js"
  },
  "dependencies": {
    "colorette": "^1.2.0",
    "globby": "^11.0.1"
  },
  "devDependencies": {
    "rollup": "2.22.1",
    "esm": "^3.2.25",
    "uvu": "^0.3.1",
    "bundt": "1.0.2"
  },
  "files": [
    "lib",
    "manager",
    "index.d.ts",
    "manager.d.ts"
  ],
  "modes": {
    "manager": "src/manager.js",
    "default": "src/index.js"
  },
  "keywords": [
    "rollup",
    "rollup-plugin",
    "offline",
    "sw",
    "service-worker"
  ],
  "license": "Unlicense"
}
